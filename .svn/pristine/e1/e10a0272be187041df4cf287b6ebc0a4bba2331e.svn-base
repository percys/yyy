<link rel="stylesheet" type="text/css" href="<%=basePath%>/jquery-easyui/themes/icon.css" />
<link rel="stylesheet" type="text/css" href="<%=basePath%>/jquery-easyui/themes/myicon.css" />
<script type="text/javascript" src="<%=basePath%>/jquery-easyui/jquery-1.10.2.min.js"></script>
<script type="text/javascript" src="<%=basePath%>/jquery-easyui/jquery.easyui.min.js"></script>
<script type="text/javascript" src="<%=basePath%>/jquery-easyui/locale/easyui-lang-zh_CN.js"></script>
<script type="text/javascript" src="<%=basePath%>/resource/commons/sys.util.js"></script>
<script type="text/javascript" src="<%=basePath%>/jquery-easyui/sha.js"></script>
